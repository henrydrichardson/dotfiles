set nocompatible              
filetype off                  


set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'
Plugin 'ctrlpvim/ctrlp.vim'
Plugin 'saltstack/salt-vim'
Plugin 'jelera/vim-javascript-syntax'
Plugin 'hashivim/vim-vagrant'

call vundle#end()            
filetype plugin indent on
color elflord
syntax on

